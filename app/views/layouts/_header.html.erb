<header>

  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">

  <a class="navbar-brand" href="/"><span>Bookers</span></a>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">

      <span class="navbar-toggler-icon"></span>

    </button>

    <div class="collapse navbar-collapse" id="navbarNavDropdown">
      
      <ul class="navbar-nav ml-auto">
        
        <% if user_signed_in? %>
      
          <li class= "nav-link text-light">
          <%= link_to(user_path(current_user.id), class: "text-light") do %>
          <strong><i class="fa-solid fa-home"></i><span>Home</span></strong>
          <% end %>
          </li>
    
          <li class= "nav-link text-light">
          <%= link_to(users_path, class: "text-light") do %>
          <strong><i class="fa-solid fa-users"></i><span>Users</span></strong>
          <% end %>
          </li>
  
          <li class= "nav-link text-light">
          <%= link_to(books_path, class: "text-light") do %>
          <strong><i class="fa-solid fa-book-open"></i><span>Books</span></strong>
          <% end %>
          </li>
          
          <li class= "nav-link text-light">
          <%= link_to(destroy_user_session_path, method: :delete, class: "text-light") do %>
          <strong><i class="fa-solid fa-sign-out-alt"></i><span>Log out</span></strong>
          <% end %>
          </li>
    
        <% else %>
    
      <li class= "nav-link text-light">
      <%= link_to(root_path, class: "text-light") do %>
      <strong><i class="fa-solid fa-home"></i><span>Home</span></strong>
      <% end %>
      </li>
    
      <li class= "nav-link text-light">
      <%= link_to(about_path, class: "text-light") do %>
      <strong><i class="fa-solid fa-link"></i><span>About</span></strong>
      <% end %>
      </li>
      
      <li class= "nav-link text-light">
      <%= link_to(new_user_registration_path, class: "text-light") do %>
       <strong><i class="fa-solid fa-user-plus"></i><span>Sign up</span></strong>
      <% end %>
      </li>
      
      <li class= "nav-link text-light">
      <%= link_to(new_user_session_path, class: "text-light") do %>
       <strong><i class="fa-solid fa-sign-in-alt"></i><span>Log in</span></strong>
      <% end %>
      </li>
  
  <% end %>
    
</ul>
     

        

</header>